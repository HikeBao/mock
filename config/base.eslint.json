{
  "rules": {
    "no-console": "warn",
    "no-debugger": "error",
    "default-case": "warn",
    "eqeqeq": ["error", "always"],
    "no-with": "error",
    "no-shadow-restricted-names": "error",
    "no-undef": ["error", {"typeof": false}],
    "no-unused-vars": "off",
    "no-empty-pattern": "warn",
    "no-empty": "warn",
    "no-use-before-define": ["error", { "functions": false, "classes": false, "variables": false }]
  }
}
